<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="report_patient_pdf" model="ir.actions.report">
        <field name="name">patient pdf report</field>
        <field name="model">task.customer</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">business_management.report_partner_document</field>
        <field name="report_file">hospital_management.report_partner_document</field>
        <!--        <field name="paperformat_id" ref="hr.paperformat_hr_employee_badge"/>-->
        <field name="print_report_name">(object.id)</field>
        <field name="binding_model_id" ref="model_task_customer"/>
        <field name="binding_type">report</field>
    </record>


    <record id="report_patient_excel" model="ir.actions.report">
        <field name="name">partner excel report</field>
        <field name="model">task.customer</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">business_management.report_partner_xls</field>
        <field name="report_file">business_management.report_partner_xls</field>
        <!--        <field name="paperformat_id" ref="hr.paperformat_hr_employee_badge"/>-->
        <field name="binding_model_id" ref="model_task_customer"/>
        <field name="binding_type">report</field>
    </record>





</odoo>

